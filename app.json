{
  "expo": {
    "name": "Koala Pro",
    "slug": "koala-bot",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/koala-bot.png",
    "scheme": "koalabot",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.koalacreative.koalabot"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/koala-android.png",
        "backgroundImage": "./assets/images/koala-background.png",
        "monochromeImage": "./assets/images/koala-android-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.koalacreative.koalabot"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/koala-splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c0302e7b-2f9c-498d-b142-c70c19c389ce"
      }
    }
  }
}
